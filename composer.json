{
    "name": "filp/whoops",
    "license": "MIT",
    "description": "php error handling for cool kids",
    "keywords": ["library", "error", "handling", "exception", "silex-provider", "whoops", "zf2"],
    "homepage": "https://github.com/filp/whoops",
